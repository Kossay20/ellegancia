      value="{{value | escape}}"
      type="radio"
      class="sr-only"
    />
    <span class="sr-only">{{value | escape}}</span>
    
      
    {% assign colorsString = null %}
    {% assign colors = null %}
    {% for label in labels %}
      {% if label == value %}
        {% assign colorsString = colorStrings[forloop.index0] %}
      {% endif %}
