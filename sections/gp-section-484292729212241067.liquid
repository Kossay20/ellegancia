    {%- endif -%}
    "
    style="
      {%- if selectedValue == value -%}
        --bg: transparent;--bs: solid;--bw: 1px 1px 1px 1px;--bc: #777777;--radius: var(--g-radius-none);--fs: normal;--ff: var(--g-font-Jost, Jost);--weight: 500;--ls: normal;--lh: 150%;--size: 16px;--size-tablet: 16px;--size-mobile: 14px;
      {%- else -%}
